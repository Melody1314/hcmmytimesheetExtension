{
  "welcomeFile": "/webapp/index.html",
  "routes": [
    {
      "path": "/resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "version": "1.28.8",
        "preferLocal": true,
        "entryPath": "/resources"
      },
      "description": "SAPUI5 Resources"
    },
    {
      "path": "/sap/public/bc/themes/UI5",
      "target": {
        "type": "service",
        "name": "sapui5",
        "version": "1.28.8",
        "preferLocal": true,
        "entryPath": "/resources"
      },
      "description": "SAPUI5 Themes"
    },
    {
      "path": "/sap/opu/odata",
      "target": {
        "type": "destination",
        "name": "FIORI_BACKEND",
        "entryPath": "/sap/opu/odata"
      },
      "description": "OData services backend"
    },
    {
      "path": "/webapp/parent",
      "target": {
        "type": "application",
        "preferLocal": true,
        "name": "hcmmytimesheet"
      },
      "description": "Original Application"
    },
    {
      "path": "/webapp/resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "version": "1.28.8",
        "preferLocal": true,
        "entryPath": "/resources"
      },
      "description": "SAPUI5 Resources"
    },
    {
      "path": "/webapp/test-resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/test-resources"
      },
      "description": "SAPUI5 Test Resources"
    },
    {
      "path": "/test-resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/test-resources"
      },
      "description": "SAPUI5 Test Resources"
    }
  ],
  "sendWelcomeFileRedirect": true,
  "cacheControl": [
    {
      "directive": "private",
      "maxAge": 0,
      "path": "*html"
    },
    {
      "directive": "public",
      "maxAge": 31536000
    }
  ]
}