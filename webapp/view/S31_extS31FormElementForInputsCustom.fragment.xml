										<sap.ui.layout.form:FormElement>
		<sap.ui.layout.form:label>
			<Label class="labelPadding" id="getLocationGPS" text="Get Location"/>
		</sap.ui.layout.form:label>
		<sap.ui.layout.form:fields>
				<Button icon="sap-icon://locate-me" press="getGPS" text="Get GPS"/>
		</sap.ui.layout.form:fields>
	</sap.ui.layout.form:FormElement>
	
	<sap.ui.layout.form:FormElement>
		<sap.ui.layout.form:label>
			<Label class="labelPadding" id="locationLabelX" text="Work location X coordinate"/>
		</sap.ui.layout.form:label>
		<sap.ui.layout.form:fields>
			<Input editable="false" enabled="true" id="locationX" placeholder="X Coordinate" value="{entry>/locationX}" type="Text"/>
		</sap.ui.layout.form:fields>
	</sap.ui.layout.form:FormElement>
	
		<sap.ui.layout.form:FormElement>
		<sap.ui.layout.form:label>
			<Label class="labelPadding" id="locationLabelY" text="Work location Y coordinate"/>
		</sap.ui.layout.form:label>
		<sap.ui.layout.form:fields>
			<Input editable="false" enabled="true" id="locationY" placeholder="Y Coordinate" type="Text"/>
		
		</sap.ui.layout.form:fields>
	</sap.ui.layout.form:FormElement>
	
	
	
